    "def leximin(x, y):\n",
    "    n = len(x)\n",
    "    x_cp = x[:]; y_cp = y[:]\n",
    "    x_cp.sort()\n",
    "    y_cp.sort()\n",
    "    k = 0\n",
    "    while k < n and x_cp[k] == y_cp[k]:\n",
    "        k += 1\n",
    "    if k == n:\n",
    "        return [x, y]\n",
    "    elif x_cp[k] < y_cp[k]:\n",
    "        return [y]\n",
    "    else:\n",
    "        return [x]\n",
    "\n",
    "# leximin([0,1],[1,0])\n",
    "# leximin([.5,1],[1.5,0])\n",
    "\n",
    "def leximin_lst(l):\n",
    "    lst_max = [l[0][:]]\n",
    "    max = l[0][:]\n",
    "    for i in range(1,len(l)):\n",
    "        if leximin(max, l[i][:]) == [l[i][:]]:\n",
    "            lst_max = [l[i][:]]\n",
    "            max = l[i][:]\n",
    "        elif leximin(max, l[i][:]) == [max, l[i][:]]:\n",
    "            lst_max.append(l[i][:])\n",
    "    return lst_max\n",
    "\n",
    "# l1 = [.5,1] ; l2 = [1,.7] ; l3 = [1.5,0] ; l4 = [.7,1]\n",
    "# leximin_lst([l1,l2,l3,l4])"
